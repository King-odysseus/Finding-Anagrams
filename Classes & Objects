class Student: 
    # [assignment] Skeleton class. Add your code here
    def __init__(self, name, age, tracks, score):
        self.name = str(name)
        self.age = int(age)
        self.tracks = str(tracks)
        self.score = float(score)
        
    def change_name(Bob, change_name):
        Bob.change_name = change_name
        print ("Changed Name" , change_name)
    
    def change_age(Bob, change_age):
        Bob.change_age = change_age
        print ("Changed Age" , change_age)
    
    def add_tracks(Bob, add_track):
        Bob.add_track = add_track
        print ("Changed Track", add_track)
    
    def get_score(self):
        return ("Changed score", self.score)

Bob = Student(name="Bob", age=26, tracks=["FE","BE"], score=20.90)

# Expected methods
Bob.change_name("Peter")
Bob.change_age(34)
Bob.add_tracks("UI/UX")
Bob.get_score()
